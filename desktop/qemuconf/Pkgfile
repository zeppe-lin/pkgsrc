# Description: Qemu launcher with config file support
# URL:         https://zeppe-lin.github.io/

name=qemuconf
version=0.2.1
release=1
source="src/config.mk
	src/Makefile
	src/qemuconf.c
	src/qemuconf.1.pod
	src/qemuconf-import
	src/qemuconf-import.1.pod"

build() {
	make PREFIX=/usr DESTDIR=$PKG install
}
